-- Animation variables
local isMinimized = false
local animationConnection

-- Animation functions
local function createPulseAnimation(object, property, startValue, endValue, duration)
    local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true)
    local tween = TweenService:Create(object, tweenInfo, {[property] = endValue})
    tween:Play()
    return tween
end

local function createFloatAnimation(object, startY, floatAmount, duration)
    local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true)
    local tween = TweenService:Create(object, tweenInfo, {Position = UDim2.new(object.Position.X.Scale, object.Position.X.Offset, object.Position.Y.Scale, startY + floatAmount)})
    tween:Play()
    return tween
end

-- Start animations
local statusPulse = createPulseAnimation(statusDot, "BackgroundTransparency", 0, 0.3, 1)
local hovercraftFloat = createFloatAnimation(hovercraftFrame, 10, -5, 2)
local light1Pulse = createPulseAnimation(statusLight1, "BackgroundTransparency", 0, 0.4, 1.5)
local light2Pulse = createPulseAnimation(statusLight2, "BackgroundTransparency", 0, 0.3, 1.2)

-- Button hover effects
local function createHoverEffect(button, hoverColor, normalColor)
    button.MouseEnter:Connect(function()
        local tween = TweenService:Create(button, TweenInfo.new(0.2), {BackgroundColor3 = hoverColor})
        tween:Play()
    end)
    
    button.MouseLeave:Connect(function()
        local tween = TweenService:Create(button, TweenInfo.new(0.2), {BackgroundColor3 = normalColor})
        tween:Play()
    end)
end

-- Apply hover effects
createHoverEffect(mainButton, Color3.fromRGB(70, 140, 255), Color3.fromRGB(50, 120, 255))
createHoverEffect(infoButton, Color3.fromRGB(80, 90, 100), Color3.fromRGB(60, 70, 80))
createHoverEffect(minimizeBtn, Color3.fromRGB(220, 170, 70), Color3.fromRGB(200, 150, 50))
createHoverEffect(closeBtn, Color3.fromRGB(220, 70, 70), Color3.fromRGB(200, 50, 50))

-- Button click effects
local function createClickEffect(button)
    button.MouseButton1Click:Connect(function()
        local originalSize = button.Size
        local shrinkTween = TweenService:Create(button, TweenInfo.new(0.1), {Size = UDim2.new(originalSize.X.Scale * 0.95, originalSize.X.Offset * 0.95, originalSize.Y.Scale * 0.95, originalSize.Y.Offset * 0.95)})
        shrinkTween:Play()
        
        shrinkTween.Completed:Connect(function()
            local expandTween = TweenService:Create(button, TweenInfo.new(0.1), {Size = originalSize})
            expandTween:Play()
        end)
    end)
end

-- Apply click effects
createClickEffect(mainButton)
createClickEffect(infoButton)
createClickEffect(minimizeBtn)
createClickEffect(closeBtn)

-- Button functionality
minimizeBtn.MouseButton1Click:Connect(function()
    isMinimized = not isMinimized
    local targetSize = isMinimized and UDim2.new(0, 400, 0, 40) or UDim2.new(0, 400, 0, 300)
    local targetTransparency = isMinimized and 0.3 or 0
    
    local sizeTween = TweenService:Create(mainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quart), {Size = targetSize})
    local transparencyTween = TweenService:Create(contentFrame, TweenInfo.new(0.3), {BackgroundTransparency = targetTransparency})
    
    sizeTween:Play()
    transparencyTween:Play()
    
    contentFrame.Visible = not isMinimized
end)

closeBtn.MouseButton1Click:Connect(function()
    local closeTween = TweenService:Create(mainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.In), {
        Size = UDim2.new(0, 0, 0, 0),
        Position = UDim2.new(0, mainFrame.Position.X.Offset + 200, 0, mainFrame.Position.Y.Offset + 150)
    })
    closeTween:Play()
    
    closeTween.Completed:Connect(function()
        screenGui:Destroy()
    end)
end)

mainButton.MouseButton1Click:Connect(function()
    -- Game launch functionality
    local notification = Instance.new("TextLabel")
    notification.Size = UDim2.new(0, 200, 0, 50)
    notification.Position = UDim2.new(0.5, -100, 0, 50)
    notification.BackgroundColor3 = Color3.fromRGB(50, 200, 100)
    notification.Text = "🚀 Launching Game..."
    notification.TextColor3 = Color3.fromRGB(255, 255, 255)
    notification.TextSize = 14
    notification.Font = Enum.Font.GothamBold
    notification.BorderSizePixel = 0
    notification.Parent = screenGui
    
    local notifCorner = Instance.new("UICorner")
    notifCorner.CornerRadius = UDim.new(0, 8)
    notifCorner.Parent = notification
    
    -- Animate notification
    local slideIn = TweenService:Create(notification, TweenInfo.new(0.3), {Position = UDim2.new(0.5, -100, 0, 100)})
    slideIn:Play()
    
    wait(2)
    
    local slideOut = TweenService:Create(notification, TweenInfo.new(0.3), {
        Position = UDim2.new(0.5, -100, 0, 50),
        BackgroundTransparency = 1,
        TextTransparency = 1
    })
    slideOut:Play()
    
    slideOut.Completed:Connect(function()
        notification:Destroy()
    end)
    
    print("🎮 [Hovercraft] War Tycoon - Game Launched!")
end)

infoButton.MouseButton1Click:Connect(function()
    infoDialog.Visible = true
    dialogOverlay.Visible = true
    
    -- Animate dialog appearance
    infoDialog.Size = UDim2.new(0, 0, 0, 0)
    infoDialog.Position = UDim2.new(0.5, 0, 0.5, 0)
    
    local openTween = TweenService:Create(infoDialog, TweenInfo.new(0.4, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
        Size = UDim2.new(0, 300, 0, 350),
        Position = UDim2.new(0.5, -150, 0.5, -175)
    })
    openTween:Play()
end)

dialogCloseBtn.MouseButton1Click:Connect(function()
    local closeTween = TweenService:Create(infoDialog, TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.In), {
        Size = UDim2.new(0, 0, 0, 0),
        Position = UDim2.new(0.5, 0, 0.5, 0)
    })
    closeTween:Play()
    
    closeTween.Completed:Connect(function()
        infoDialog.Visible = false
        dialogOverlay.Visible = false
    end)
end)

dialogOverlay.MouseButton1Click:Connect(function()
    dialogCloseBtn.MouseButton1Click:Fire()
end)

-- Update shadow position when main frame moves
local function updateShadow()
    shadowFrame.Position = UDim2.new(0, mainFrame.Position.X.Offset + 5, 0, mainFrame.Position.Y.Offset + 5)
end

-- Connect shadow update to frame movement
RunService.Heartbeat:Connect(updateShadow)

-- Keyboard shortcuts
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    
    if input.KeyCode == Enum.KeyCode.Space then
        mainButton.MouseButton1Click:Fire()
    elseif input.KeyCode == Enum.KeyCode.I then
        infoButton.MouseButton1Click:Fire()
    elseif input.KeyCode == Enum.KeyCode.Escape then
        if infoDialog.Visible then
            dialogCloseBtn.MouseButton1Click:Fire()
        else
            closeBtn.MouseButton1Click:Fire()
        end
    end
end)

-- Welcome message
wait(0.5)
print("🚁 [Hovercraft] War Tycoon Launcher Loaded!")
print("📋 Controls:")
print("   • Drag window to move")
print("   • Space: Launch game")
print("   • I: Toggle info")
print("   • Escape: Close")
print("⚡ System Ready!")
