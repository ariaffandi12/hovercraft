    closeBtn.Parent = titleBar
    
    local closeBtnCorner = Instance.new("UICorner")
    closeBtnCorner.CornerRadius = UDim.new(0, 3)
    closeBtnCorner.Parent = closeBtn
    
    -- Scroll Frame
    local scrollFrame = Instance.new("ScrollingFrame")
    scrollFrame.Size = UDim2.new(1, -20, 1, -60)
    scrollFrame.Position = UDim2.new(0, 10, 0, 50)
    scrollFrame.BackgroundTransparency = 1
    scrollFrame.BorderSizePixel = 0
    scrollFrame.ScrollBarThickness = 6
    scrollFrame.ScrollBarImageColor3 = Color3.fromRGB(70, 130, 255)
    scrollFrame.CanvasSize = UDim2.new(0, 0, 0, 800)
    scrollFrame.Parent = mainFrame
    
    local layout = Instance.new("UIListLayout")
    layout.SortOrder = Enum.SortOrder.LayoutOrder
    layout.Padding = UDim.new(0, 10)
    layout.Parent = scrollFrame
    
    -- Fungsi buat toggle button
    local function createToggleButton(name, description, callback)
        local buttonFrame = Instance.new("Frame")
        buttonFrame.Size = UDim2.new(1, -20, 0, 60)
        buttonFrame.BackgroundColor3 = Color3.fromRGB(40, 45, 55)
        buttonFrame.BorderSizePixel = 0
        buttonFrame.Parent = scrollFrame
        
        local buttonCorner = Instance.new("UICorner")
        buttonCorner.CornerRadius = UDim.new(0, 8)
        buttonCorner.Parent = buttonFrame
        
        local nameLabel = Instance.new("TextLabel")
        nameLabel.Size = UDim2.new(0, 250, 0, 25)
        nameLabel.Position = UDim2.new(0, 15, 0, 8)
        nameLabel.BackgroundTransparency = 1
        nameLabel.Text = name
        nameLabel.TextColor3 = Color3.fromRGB(200, 220, 255)
        nameLabel.TextSize = 14
        nameLabel.Font = Enum.Font.GothamBold
        nameLabel.TextXAlignment = Enum.TextXAlignment.Left
        nameLabel.Parent = buttonFrame
        
        local descLabel = Instance.new("TextLabel")
        descLabel.Size = UDim2.new(0, 250, 0, 20)
        descLabel.Position = UDim2.new(0, 15, 0, 30)
        descLabel.BackgroundTransparency = 1
        descLabel.Text = description
        descLabel.TextColor3 = Color3.fromRGB(150, 170, 190)
        descLabel.TextSize = 10
        descLabel.Font = Enum.Font.Gotham
        descLabel.TextXAlignment = Enum.TextXAlignment.Left
        descLabel.Parent = buttonFrame
        
        local toggleBtn = Instance.new("TextButton")
        toggleBtn.Size = UDim2.new(0, 80, 0, 30)
        toggleBtn.Position = UDim2.new(1, -90, 0, 15)
        toggleBtn.BackgroundColor3 = Color3.fromRGB(60, 70, 80)
        toggleBtn.Text = "OFF"
        toggleBtn.TextColor3 = Color3.fromRGB(255, 100, 100)
        toggleBtn.TextSize = 12
        toggleBtn.Font = Enum.Font.GothamBold
        toggleBtn.BorderSizePixel = 0
        toggleBtn.Parent = buttonFrame
        
        local toggleCorner = Instance.new("UICorner")
        toggleCorner.CornerRadius = UDim.new(0, 6)
        toggleCorner.Parent = toggleBtn
        
        local isEnabled = false
        toggleBtn.MouseButton1Click:Connect(function()
            isEnabled = not isEnabled
            if isEnabled then
                toggleBtn.Text = "ON"
                toggleBtn.TextColor3 = Color3.fromRGB(100, 255, 100)
                toggleBtn.BackgroundColor3 = Color3.fromRGB(50, 120, 50)
            else
                toggleBtn.Text = "OFF"
                toggleBtn.TextColor3 = Color3.fromRGB(255, 100, 100)
                toggleBtn.BackgroundColor3 = Color3.fromRGB(60, 70, 80)
            end
            callback(isEnabled)
        end)
        
        return toggleBtn
    end
    
    -- Fungsi buat action button
    local function createActionButton(name, description, callback)
        local buttonFrame = Instance.new("Frame")
        buttonFrame.Size = UDim2.new(1, -20, 0, 60)
        buttonFrame.BackgroundColor3 = Color3.fromRGB(40, 45, 55)
        buttonFrame.BorderSizePixel = 0
        buttonFrame.Parent = scrollFrame
        
        local buttonCorner = Instance.new("UICorner")
        buttonCorner.CornerRadius = UDim.new(0, 8)
        buttonCorner.Parent = buttonFrame
        
        local nameLabel = Instance.new("TextLabel")
        nameLabel.Size = UDim2.new(0, 250, 0, 25)
        nameLabel.Position = UDim2.new(0, 15, 0, 8)
        nameLabel.BackgroundTransparency = 1
        nameLabel.Text = name
        nameLabel.TextColor3 = Color3.fromRGB(200, 220, 255)
        nameLabel.TextSize = 14
        nameLabel.Font = Enum.Font.GothamBold
        nameLabel.TextXAlignment = Enum.TextXAlignment.Left
        nameLabel.Parent = buttonFrame
        
        local descLabel = Instance.new("TextLabel")
        descLabel.Size = UDim2.new(0, 250, 0, 20)
        descLabel.Position = UDim2.new(0, 15, 0, 30)
        descLabel.BackgroundTransparency = 1
        descLabel.Text = description
        descLabel.TextColor3 = Color3.fromRGB(150, 170, 190)
        descLabel.TextSize = 10
        descLabel.Font = Enum.Font.Gotham
        descLabel.TextXAlignment = Enum.TextXAlignment.Left
        descLabel.Parent = buttonFrame
        
        local actionBtn = Instance.new("TextButton")
        actionBtn.Size = UDim2.new(0, 80, 0, 30)
        actionBtn.Position = UDim2.new(1, -90, 0, 15)
        actionBtn.BackgroundColor3 = Color3.fromRGB(50, 120, 255)
        actionBtn.Text = "EXECUTE"
        actionBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
        actionBtn.TextSize = 10
        actionBtn.Font = Enum.Font.GothamBold
        actionBtn.BorderSizePixel = 0
        actionBtn.Parent = buttonFrame
        
        local actionCorner = Instance.new("UICorner")
        actionCorner.CornerRadius = UDim.new(0, 6)
        actionCorner.Parent = actionBtn
        
        actionBtn.MouseButton1Click:Connect(callback)
        
        return actionBtn
    end
    
    -- Buat semua tombol fitur
    createToggleButton("💰 Auto Collect Money", "Otomatis mengumpulkan uang dari tycoon", function(enabled)
        HovercraftHack.features.autoCollectMoney = enabled
        if enabled then
            HovercraftHack.connections.autoMoneyConnection = RunService.Heartbeat:Connect(autoCollectMoney)
            createNotification("💰 Auto Collect Money diaktifkan!", Color3.fromRGB(100, 255, 100))
        else
            if HovercraftHack.connections.autoMoneyConnection then
                HovercraftHack.connections.autoMoneyConnection:Disconnect()
            end
            createNotification("💰 Auto Collect Money dimatikan!", Color3.fromRGB(255, 100, 100))
        end
    end)
    
    createToggleButton("⬆️ Auto Upgrade", "Otomatis upgrade semua building", function(enabled)
        HovercraftHack.features.autoUpgrade = enabled
        if enabled then
            HovercraftHack.connections.autoUpgradeConnection = RunService.Heartbeat:Connect(autoUpgrade)
            createNotification("⬆️ Auto Upgrade diaktifkan!", Color3.fromRGB(100, 255, 100))
        else
            if HovercraftHack.connections.autoUpgradeConnection then
                HovercraftHack.connections.autoUpgradeConnection:Disconnect()
            end
            createNotification("⬆️ Auto Upgrade dimatikan!", Color3.fromRGB(255, 100, 100))
        end
    end)
    
    createToggleButton("💎 Infinite Money", "Uang tidak terbatas", function(enabled)
        HovercraftHack.features.infiniteMoney = enabled
        if enabled then
            HovercraftHack.connections.infiniteMoneyConnection = RunService.Heartbeat:Connect(infiniteMoney)
            createNotification("💎 Infinite Money diaktifkan!", Color3.fromRGB(255, 215, 0))
        else
            if HovercraftHack.connections.infiniteMoneyConnection then
                HovercraftHack.connections.infiniteMoneyConnection:Disconnect()
            end
            createNotification("💎 Infinite Money dimatikan!", Color3.fromRGB(255, 100, 100))
        end
    end)
    
    createToggleButton("⚡ Speed Hack", "Kecepatan berjalan super cepat", function(enabled)
        HovercraftHack.features.speedHack = enabled
        speedHack(enabled)
        if enabled then
            createNotification("⚡ Speed Hack diaktifkan!", Color3.fromRGB(255, 255, 100))
        else
            createNotification("⚡ Speed Hack dimatikan!", Color3.fromRGB(255, 100, 100))
        end
    end)
    
    createToggleButton("🚀 Fly Hack", "Terbang bebas (Space/Shift)", function(enabled)
        HovercraftHack.features.flyHack = enabled
        flyHack(enabled)
        if enabled then
            createNotification("🚀 Fly Hack diaktifkan! Space = naik, Shift = turun", Color3.fromRGB(100, 200, 255))
        else
            createNotification("🚀 Fly Hack dimatikan!", Color3.fromRGB(255, 100, 100))
        end
    end)
    
    createToggleButton("👻 NoClip", "Tembus dinding dan objek", function(enabled)
        HovercraftHack.features.noclip = enabled
        noclip(enabled)
        if enabled then
            createNotification("👻 NoClip diaktifkan!", Color3.fromRGB(200, 100, 255))
        else
            createNotification("👻 NoClip dimatikan!", Color3.fromRGB(255, 100, 100))
        end
    end)
    
    createToggleButton("👁️ ESP", "Lihat player dan hovercraft melalui dinding", function(enabled)
        HovercraftHack.features.esp = enabled
        toggleESP(enabled)
        if enabled then
            createNotification("👁️ ESP diaktifkan!", Color3.fromRGB(255, 150, 100))
        else
            createNotification("👁️ ESP dimatikan!", Color3.fromRGB(255, 100, 100))
        end
    end)
    
    createToggleButton("🏴 Auto Capture", "Otomatis capture point terdekat", function(enabled)
        HovercraftHack.features.autoCapture = enabled
        if enabled then
            HovercraftHack.connections.autoCaptureConnection = RunService.Heartbeat:Connect(autoCapture)
            createNotification("🏴 Auto Capture diaktifkan!", Color3.fromRGB(255, 100, 150))
        else
            if HovercraftHack.connections.autoCaptureConnection then
                HovercraftHack.connections.autoCaptureConnection:Disconnect()
            end
            createNotification("🏴 Auto Capture dimatikan!", Color3.fromRGB(255, 100, 100))
        end
    end)
    
    -- Action Buttons
    createActionButton("🏠 Teleport to Base", "Teleport ke base tycoon Anda", teleportToBase)
    
    createActionButton("🔄 Refresh Detection", "Refresh deteksi elemen game", function()
        detectGameElements()
        createNotification("🔄 Game elements di-refresh!", Color3.fromRGB(100, 200, 255))
    end)
    
    -- Close button functionality
    closeBtn.MouseButton1Click:Connect(function()
        screenGui:Destroy()
    end)
    
    HovercraftHack.gui = screenGui
    return screenGui
end

-- Inisialisasi
local function initialize()
    createNotification("🚁 Hovercrafts War Tycoon Hack dimuat!", Color3.fromRGB(100, 200, 255))
    createGUI()
    
    -- Deteksi elemen game
    detectGameElements()
    
    print("🚁 Hovercrafts War Tycoon Hack berhasil dimuat!")
    print("📋 Fitur yang tersedia:")
    print("   • Auto Collect Money")
    print("   • Auto Upgrade")
    print("   • Infinite Money")
    print("   • Speed Hack")
    print("   • Fly Hack")
    print("   • NoClip")
    print("   • ESP")
    print("   • Auto Capture")
    print("   • Teleport to Base")
    print("⚡ Selamat bermain!")
end

-- Jalankan script
initialize()

-- Keyboard shortcut untuk toggle GUI
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    
    if input.KeyCode == Enum.KeyCode.Insert then
        if HovercraftHack.gui then
            HovercraftHack.gui.MainFrame.Visible = not HovercraftHack.gui.MainFrame.Visible
        end
    end
end)
