-- Info text content
local infoTexts = {
    {label = "Game:", value = "[Hovercraft] War Tycoon", color = Color3.fromRGB(100, 200, 255)},
    {label = "Version:", value = "v2.1.0", color = Color3.fromRGB(100, 255, 200)},
    {label = "Developer:", value = "Future Tech Studios", color = Color3.fromRGB(255, 200, 100)},
    {label = "Engine:", value = "Quantum Core 3D", color = Color3.fromRGB(200, 100, 255)},
    {label = "Platform:", value = "Roblox Executor", color = Color3.fromRGB(255, 150, 150)},
}

for i, info in ipairs(infoTexts) do
    local infoFrame = Instance.new("Frame")
    infoFrame.Size = UDim2.new(1, 0, 0, 25)
    infoFrame.Position = UDim2.new(0, 0, 0, (i-1) * 30)
    infoFrame.BackgroundTransparency = 1
    infoFrame.Parent = dialogContent
    
    local labelText = Instance.new("TextLabel")
    labelText.Size = UDim2.new(0, 80, 1, 0)
    labelText.Position = UDim2.new(0, 0, 0, 0)
    labelText.BackgroundTransparency = 1
    labelText.Text = info.label
    labelText.TextColor3 = Color3.fromRGB(150, 170, 190)
    labelText.TextSize = 12
    labelText.Font = Enum.Font.Gotham
    labelText.TextXAlignment = Enum.TextXAlignment.Left
