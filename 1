-- [Hovercraft] War Tycoon Launcher for Roblox
-- Single file executor script

local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local SoundService = game:GetService("SoundService")

local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- Clean up existing launcher
if playerGui:FindFirstChild("HovercraftLauncher") then
    playerGui.HovercraftLauncher:Destroy()
end

-- Create main ScreenGui
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "HovercraftLauncher"
screenGui.ResetOnSpawn = false
screenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
screenGui.Parent = playerGui

-- Main launcher window
local mainFrame = Instance.new("Frame")
mainFrame.Name = "LauncherWindow"
mainFrame.Size = UDim2.new(0, 400, 0, 300)
mainFrame.Position = UDim2.new(0, 200, 0, 150)
mainFrame.BackgroundColor3 = Color3.fromRGB(25, 30, 40)
mainFrame.BorderSizePixel = 0
mainFrame.Active = true
mainFrame.Draggable = true
mainFrame.Parent = screenGui

-- Window corner rounding
